<?xml version="1.0" encoding="ISO-8859-1"?>
<ruletypesmm:Trafo
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:ruletypesmm="http://ruletypesmm/1.0"
    name="Families2Persons_Extended">
  <rules name="Country2Community">
    <inTypes>src_Country</inTypes>
    <outTypes>trg_Community</outTypes>
    <allNavigationPaths>src_Country</allNavigationPaths>
    <allNavigationPaths>trg_Community</allNavigationPaths>
    <allNavigationPaths>src_Country.families.fathers</allNavigationPaths>
    <allNavigationPaths>src_Country.families.mothers</allNavigationPaths>
    <allNavigationPaths>src_Country.families.sons</allNavigationPaths>
    <allNavigationPaths>src_Country.families.daughters</allNavigationPaths>
    <allNavigationPaths>src_Country.cities</allNavigationPaths>
    <allNavigationPaths>trg_Community.persons</allNavigationPaths>
    <allNavigationPaths>trg_Community.townHalls</allNavigationPaths>
    <allNavigationPaths>trg_Community.associations</allNavigationPaths>
    <bindingFeatures>trg_Community.persons</bindingFeatures>
    <bindingFeatures>trg_Community.persons</bindingFeatures>
    <bindingFeatures>trg_Community.persons</bindingFeatures>
    <bindingFeatures>trg_Community.persons</bindingFeatures>
    <bindingFeatures>trg_Community.townHalls</bindingFeatures>
    <bindingFeatures>trg_Community.associations</bindingFeatures>
    <allFootPrints>src_Country</allFootPrints>
    <allFootPrints>trg_Community</allFootPrints>
    <allFootPrints>src_Family.fathers</allFootPrints>
    <allFootPrints>src_Family</allFootPrints>
    <allFootPrints>src_Parent</allFootPrints>
    <allFootPrints>src_Family.mothers</allFootPrints>
    <allFootPrints>src_Family.sons</allFootPrints>
    <allFootPrints>src_Child</allFootPrints>
    <allFootPrints>src_Family.daughters</allFootPrints>
    <allFootPrints>src_Country.cities</allFootPrints>
    <allFootPrints>src_City</allFootPrints>
    <allFootPrints>trg_Community.persons</allFootPrints>
    <allFootPrints>trg_Person</allFootPrints>
    <allFootPrints>trg_Community.townHalls</allFootPrints>
    <allFootPrints>trg_TownHall</allFootPrints>
    <allFootPrints>trg_Community.associations</allFootPrints>
    <allFootPrints>trg_Association</allFootPrints>
    <outFeatures>src_Country.families.fathers</outFeatures>
    <outFeatures>src_Country.families.mothers</outFeatures>
    <outFeatures>src_Country.families.sons</outFeatures>
    <outFeatures>src_Country.families.daughters</outFeatures>
    <outFeatures>src_Country.cities</outFeatures>
    <outFeatures>src_Country.cities.companies</outFeatures>
  </rules>
  <rules name="Father2Man">
    <inFeatures>src_Parent.family.fathers</inFeatures>
    <inTypes>src_Parent</inTypes>
    <outTypes>trg_Man</outTypes>
    <allNavigationPaths>src_Parent</allNavigationPaths>
    <allNavigationPaths>src_Parent.family.fathers</allNavigationPaths>
    <allNavigationPaths>trg_Man</allNavigationPaths>
    <allNavigationPaths>src_Parent.firstName</allNavigationPaths>
    <allNavigationPaths>src_Parent.family.lastName</allNavigationPaths>
    <allNavigationPaths>trg_Man.fullName</allNavigationPaths>
    <bindingFeatures>trg_Man.fullName</bindingFeatures>
    <allFootPrints>src_Parent</allFootPrints>
    <allFootPrints>trg_Man</allFootPrints>
    <allFootPrints>trg_Man.fullName</allFootPrints>
    <outFeatures>src_Parent.firstName</outFeatures>
    <outFeatures>src_Parent.family.lastName</outFeatures>
  </rules>
  <rules name="Mother2Woman">
    <inFeatures>src_Parent.family.mothers</inFeatures>
    <inTypes>src_Parent</inTypes>
    <outTypes>trg_Woman</outTypes>
    <allNavigationPaths>src_Parent</allNavigationPaths>
    <allNavigationPaths>src_Parent.family.mothers</allNavigationPaths>
    <allNavigationPaths>trg_Woman</allNavigationPaths>
    <allNavigationPaths>src_Parent.firstName</allNavigationPaths>
    <allNavigationPaths>src_Parent.family.lastName</allNavigationPaths>
    <allNavigationPaths>trg_Woman.fullName</allNavigationPaths>
    <bindingFeatures>trg_Woman.fullName</bindingFeatures>
    <allFootPrints>src_Parent</allFootPrints>
    <allFootPrints>trg_Woman</allFootPrints>
    <allFootPrints>trg_Woman.fullName</allFootPrints>
    <outFeatures>src_Parent.firstName</outFeatures>
    <outFeatures>src_Parent.family.lastName</outFeatures>
  </rules>
  <rules name="Daughter2Woman">
    <inFeatures>src_Child.family.daughters</inFeatures>
    <inTypes>src_Child</inTypes>
    <outTypes>trg_Woman</outTypes>
    <allNavigationPaths>src_Child</allNavigationPaths>
    <allNavigationPaths>src_Child.family.daughters</allNavigationPaths>
    <allNavigationPaths>trg_Woman</allNavigationPaths>
    <allNavigationPaths>src_Child.firstName</allNavigationPaths>
    <allNavigationPaths>src_Child.family.lastName</allNavigationPaths>
    <allNavigationPaths>trg_Woman.fullName</allNavigationPaths>
    <bindingFeatures>trg_Woman.fullName</bindingFeatures>
    <allFootPrints>src_Child</allFootPrints>
    <allFootPrints>trg_Woman</allFootPrints>
    <allFootPrints>trg_Woman.fullName</allFootPrints>
    <outFeatures>src_Child.firstName</outFeatures>
    <outFeatures>src_Child.family.lastName</outFeatures>
  </rules>
  <rules name="Son2Man">
    <inFeatures>src_Child.family.sons</inFeatures>
    <inTypes>src_Child</inTypes>
    <outTypes>trg_Man</outTypes>
    <allNavigationPaths>src_Child</allNavigationPaths>
    <allNavigationPaths>src_Child.family.sons</allNavigationPaths>
    <allNavigationPaths>trg_Man</allNavigationPaths>
    <allNavigationPaths>src_Child.firstName</allNavigationPaths>
    <allNavigationPaths>src_Child.family.lastName</allNavigationPaths>
    <allNavigationPaths>trg_Man.fullName</allNavigationPaths>
    <bindingFeatures>trg_Man.fullName</bindingFeatures>
    <allFootPrints>src_Child</allFootPrints>
    <allFootPrints>trg_Man</allFootPrints>
    <allFootPrints>trg_Man.fullName</allFootPrints>
    <outFeatures>src_Child.firstName</outFeatures>
    <outFeatures>src_Child.family.lastName</outFeatures>
  </rules>
  <rules name="City2TownHall">
    <inTypes>src_City</inTypes>
    <outTypes>trg_TownHall</outTypes>
    <outTypes>trg_Committee</outTypes>
    <allNavigationPaths>src_City</allNavigationPaths>
    <allNavigationPaths>trg_TownHall</allNavigationPaths>
    <allNavigationPaths>trg_Committee</allNavigationPaths>
    <allNavigationPaths>src_City.name</allNavigationPaths>
    <allNavigationPaths>src_City.companies.employees.livesIn</allNavigationPaths>
    <allNavigationPaths>src_City.neighborhoods</allNavigationPaths>
    <allNavigationPaths>trg_TownHall.name</allNavigationPaths>
    <allNavigationPaths>trg_TownHall.workers</allNavigationPaths>
    <allNavigationPaths>trg_TownHall.committee</allNavigationPaths>
    <allNavigationPaths>trg_TownHall.districts</allNavigationPaths>
    <allNavigationPaths>trg_Committee.name</allNavigationPaths>
    <bindingFeatures>trg_TownHall.name</bindingFeatures>
    <bindingFeatures>trg_TownHall.workers</bindingFeatures>
    <bindingFeatures>trg_TownHall.committee</bindingFeatures>
    <bindingFeatures>trg_TownHall.districts</bindingFeatures>
    <bindingFeatures>trg_Committee.name</bindingFeatures>
    <allFootPrints>src_City</allFootPrints>
    <allFootPrints>trg_TownHall</allFootPrints>
    <allFootPrints>trg_Committee</allFootPrints>
    <allFootPrints>src_Parent</allFootPrints>
    <allFootPrints>src_City.neighborhoods</allFootPrints>
    <allFootPrints>src_Neighborhood</allFootPrints>
    <allFootPrints>trg_TownHall.name</allFootPrints>
    <allFootPrints>trg_TownHall.workers</allFootPrints>
    <allFootPrints>trg_Person</allFootPrints>
    <allFootPrints>trg_TownHall.committee</allFootPrints>
    <allFootPrints>trg_TownHall.districts</allFootPrints>
    <allFootPrints>trg_District</allFootPrints>
    <allFootPrints>trg_Committee.name</allFootPrints>
    <outFeatures>src_City.name</outFeatures>
    <outFeatures>src_City.companies.employees.livesIn</outFeatures>
    <outFeatures>trg_Committee</outFeatures>
    <outFeatures>src_City.neighborhoods</outFeatures>
    <outFeatures>src_City.name</outFeatures>
  </rules>
  <rules name="CityCompany2Association">
    <inFeatures>src_City.companies</inFeatures>
    <inTypes>src_City</inTypes>
    <inTypes>src_Company</inTypes>
    <outTypes>trg_Association</outTypes>
    <allNavigationPaths>src_City</allNavigationPaths>
    <allNavigationPaths>src_Company</allNavigationPaths>
    <allNavigationPaths>src_City.companies</allNavigationPaths>
    <allNavigationPaths>trg_Association</allNavigationPaths>
    <allNavigationPaths>src_City.name</allNavigationPaths>
    <allNavigationPaths>src_Company.name</allNavigationPaths>
    <allNavigationPaths>trg_Association.name</allNavigationPaths>
    <bindingFeatures>trg_Association.name</bindingFeatures>
    <bindingFeatures>trg_Association.committee</bindingFeatures>
    <allFootPrints>src_City</allFootPrints>
    <allFootPrints>src_Company</allFootPrints>
    <allFootPrints>src_City.companies</allFootPrints>
    <allFootPrints>trg_Association</allFootPrints>
    <allFootPrints>trg_Association.name</allFootPrints>
    <outFeatures>src_City.name</outFeatures>
    <outFeatures>src_Company.isIn</outFeatures>
  </rules>
  <rules name="Neighborhood2District">
    <inFeatures>src_Family.registeredIn</inFeatures>
    <inTypes>src_Neighborhood</inTypes>
    <outTypes>trg_District</outTypes>
    <allNavigationPaths>src_Neighborhood</allNavigationPaths>
    <allNavigationPaths>src_Family</allNavigationPaths>
    <allNavigationPaths>src_Family.registeredIn</allNavigationPaths>
    <allNavigationPaths>trg_District</allNavigationPaths>
    <allNavigationPaths>src_Neighborhood.name</allNavigationPaths>
    <allNavigationPaths>trg_OrdinaryFacility</allNavigationPaths>
    <allNavigationPaths>trg_SpecialFacility</allNavigationPaths>
    <allNavigationPaths>trg_District.name</allNavigationPaths>
    <allNavigationPaths>trg_District.facilities</allNavigationPaths>
    <bindingFeatures>trg_District.name</bindingFeatures>
    <bindingFeatures>trg_District.facilities</bindingFeatures>
    <bindingFeatures>trg_District.facilities</bindingFeatures>
    <allFootPrints>src_Neighborhood</allFootPrints>
    <allFootPrints>src_Family</allFootPrints>
    <allFootPrints>src_Family.registeredIn</allFootPrints>
    <allFootPrints>trg_District</allFootPrints>
    <allFootPrints>trg_OrdinaryFacility</allFootPrints>
    <allFootPrints>trg_SpecialFacility</allFootPrints>
    <allFootPrints>trg_District.name</allFootPrints>
    <allFootPrints>trg_District.facilities</allFootPrints>
    <allFootPrints>trg_Facility</allFootPrints>
    <outFeatures>src_Neighborhood.name</outFeatures>
    <outFeatures>src_Neighborhood.schools._ordinary.createOrdinaryFacility</outFeatures>
    <outFeatures>src_Neighborhood.schools._special.createSpecialFacility</outFeatures>
  </rules>
  <rules name="createOrdinaryFacility">
    <inTypes>src_School</inTypes>
    <outTypes>trg_OrdinaryFacility</outTypes>
    <allNavigationPaths>src_School</allNavigationPaths>
    <allNavigationPaths>trg_OrdinaryFacility</allNavigationPaths>
    <allNavigationPaths>src_School.name</allNavigationPaths>
    <allNavigationPaths>src_School.students</allNavigationPaths>
    <allNavigationPaths>trg_OrdinaryFacility.name</allNavigationPaths>
    <allNavigationPaths>trg_OrdinaryFacility.members</allNavigationPaths>
    <bindingFeatures>trg_OrdinaryFacility.name</bindingFeatures>
    <bindingFeatures>trg_OrdinaryFacility.members</bindingFeatures>
    <allFootPrints>src_School</allFootPrints>
    <allFootPrints>trg_OrdinaryFacility</allFootPrints>
    <allFootPrints>src_School.students</allFootPrints>
    <allFootPrints>src_Child</allFootPrints>
    <allFootPrints>trg_OrdinaryFacility.name</allFootPrints>
    <allFootPrints>trg_OrdinaryFacility.members</allFootPrints>
    <allFootPrints>trg_Person</allFootPrints>
    <outFeatures>src_School.name</outFeatures>
    <outFeatures>src_School.students</outFeatures>
  </rules>
  <rules name="createSpecialFacility">
    <inTypes>src_School</inTypes>
    <outTypes>trg_SpecialFacility</outTypes>
    <allNavigationPaths>src_School</allNavigationPaths>
    <allNavigationPaths>trg_SpecialFacility</allNavigationPaths>
    <allNavigationPaths>src_School.name</allNavigationPaths>
    <allNavigationPaths>src_School.students</allNavigationPaths>
    <allNavigationPaths>trg_SpecialFacility.name</allNavigationPaths>
    <allNavigationPaths>trg_SpecialFacility.members</allNavigationPaths>
    <bindingFeatures>trg_SpecialFacility.name</bindingFeatures>
    <bindingFeatures>trg_SpecialFacility.members</bindingFeatures>
    <allFootPrints>src_School</allFootPrints>
    <allFootPrints>trg_SpecialFacility</allFootPrints>
    <allFootPrints>src_School.students</allFootPrints>
    <allFootPrints>src_Child</allFootPrints>
    <allFootPrints>trg_SpecialFacility.name</allFootPrints>
    <allFootPrints>trg_SpecialFacility.members</allFootPrints>
    <allFootPrints>trg_Person</allFootPrints>
    <outFeatures>src_School.name</outFeatures>
    <outFeatures>src_School.students</outFeatures>
  </rules>
  <mms inputMM="families"/>
  <mms outputMM="persons"/>
</ruletypesmm:Trafo>
